// Place your key bindings in this file to overwrite the defaults
[
    // Navigation
    { "key": "ctrl+j",                  "command": "cursorWordStartLeft",   "when": "textInputFocus" },
    { "key": "ctrl+l",                  "command": "cursorWordStartRight",   "when": "textInputFocus" },
    { "key": "ctrl+i",                  "command": "cursorUp",   "when": "textInputFocus" },
    { "key": "ctrl+k",                  "command": "cursorDown", "when": "textInputFocus" },
    { "key": "alt+j",                   "command": "cursorHome", "when": "textInputFocus"},
    { "key": "alt+l",                   "command": "cursorEnd", "when": "textInputFocus"},
    { "key": "ctrl+oem_5",              "command": "editor.action.jumpToBracket", "when": "editorTextFocus"},
    { "key": "ctrl+alt+j",                      "command": "cursorLeft", "when": "textInputFocus" },
    { "key": "ctrl+alt+l",                     "command": "cursorRight", "when": "textInputFocus"},
    { "key": "f9",                      "command": "cursorTop", "when": "textInputFocus" },
    { "key": "f10",                     "command": "cursorBottom", "when": "textInputFocus"},
    
    // Select
    { "key": "ctrl+shift+i",            "command": "cursorUpSelect",   "when": "textInputFocus" },
    { "key": "alt+shift+i",            "command": "cursorUpSelect",   "when": "textInputFocus" },
    { "key": "ctrl+shift+k",            "command": "cursorDownSelect",       "when": "textInputFocus" },
    { "key": "ctrl+shift+j",            "command": "cursorWordLeftSelect",   "when": "textInputFocus" },
    { "key": "ctrl+shift+alt+l",        "command": "cursorRightSelect", "when": "textInputFocus"},
    { "key": "ctrl+shift+alt+j",        "command": "cursorLeftSelect", "when": "textInputFocus"},
    { "key": "ctrl+shift+oem_5",        "command": "editor.action.selectToBracket"},
    { "key": "alt+shift+j",             "command": "cursorHomeSelect", "when": "textInputFocus"},
    { "key": "alt+shift+l",             "command": "cursorEndSelect", "when": "textInputFocus"},
    { "key": "alt+shift+k",             "command": "expandLineSelection", "when": "textInputFocus"},
    
    // Modification
    { "key": "alt+i",                   "command": "editor.action.moveLinesUpAction", "when": "editorTextFocus && !editorReadonly"},
    { "key": "alt+k",                   "command": "editor.action.moveLinesDownAction", "when": "editorTextFocus && !editorReadonly"},
    { "key": "ctrl+shift+backspace",    "command": "editor.action.deleteLines", "when": "textInputFocus && !editorReadonly"},
    
    // Editor Groups
    { "key": "ctrl+oem_1",              "command": "workbench.action.splitEditor"},


    // Various
    { "key": "ctrl+m ctrl+k",           "command": "workbench.action.openGlobalKeybindings"},
    { "key": "ctrl+e",                  "command": "editor.action.goToDeclaration", "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"},
    { "key": "ctrl+k",                  "command": "workbench.action.quickOpenSelectNext", "when": "inQuickOpen"},
    { "key": "ctrl+i",                  "command": "workbench.action.quickOpenSelectPrevious", "when": "inQuickOpen"},

    // Removed Keybindings:
    { "key": "ctrl+e",                  "command": "-workbench.action.quickOpen" },
    { "key": "ctrl+e",                  "command": "-workbench.action.quickOpenNavigateNextInFilePicker", "when": "inFilesPicker && inQuickOpen" },
    { "key": "ctrl+shift+l",            "command": "-editor.action.selectHighlights", "when": "editorFocus" },
    { "key": "ctrl+i",                  "command": "-expandLineSelection", "when": "textInputFocus" },
    { "key": "ctrl+oem_5",              "command": "-workbench.action.splitEditor"},
    { "key": "ctrl+shift+oem_5",        "command": "-editor.action.jumpToBracket", "when": "editorTextFocus"},
    { "key": "ctrl+k ctrl+s",           "command": "-workbench.action.openGlobalKeybindings"},
    { "key": "f12",                     "command": "-editor.action.goToDeclaration", "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"},
    {
        "key": "ctrl+shift+k",
        "command": "-editor.action.deleteLines",
        "when": "textInputFocus && !editorReadonly"
    },
    {
        "key": "ctrl+k",
        "command": "list.focusDown",
        "when": "listFocus && !inputFocus"
    },
    {
        "key": "ctrl+i",
        "command": "list.focusUp",
        "when": "listFocus && !inputFocus"
    },
    {
        "key": "home",
        "command": "-cursorHome",
        "when": "textInputFocus"
    },
    {
        "key": "end",
        "command": "-cursorEnd",
        "when": "textInputFocus"
    },
    {
        "key": "shift+alt+i",
        "command": "-editor.action.insertCursorAtEndOfEachLineSelected",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+k",
        "command": "selectNextSuggestion",
        "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
    },
    {
        "key": "ctrl+i",
        "command": "selectPrevSuggestion",
        "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
    }
]